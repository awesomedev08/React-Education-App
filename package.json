{
  "name": "final-education-app",
  "version": "1.0.0",
  "scripts": {
    "server": "cd server && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail  \"yarn server\" \"yarn client\""
  },
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "formidable": "^1.2.1",
    "fs": "0.0.1-security",
    "mongoose": "^5.1.5",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
